<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <CustomAction Id="aa8deb01-4de7-4e36-9c1e-37b06af971d8.MoveDocumentsRibbonCustomAction"
                RegistrationType="List"
                RegistrationId="101"
                Location="CommandUI.Ribbon"
                Sequence="10001"
                Title="Move documents"
                HostWebDialog="TRUE"
                HostWebDialogWidth="900"
                HostWebDialogHeight="900">
    <CommandUIExtension>
      <!-- 
      Update the UI definitions below with the controls and the command actions
      that you want to enable for the custom action.
      -->
      <CommandUIDefinitions>
        <CommandUIDefinition Location="Ribbon.Documents.Copies.Controls._children">
          <Button Id="Ribbon.ListItem.Actions.MoveDocumentsRibbonCustomActionButton"
                  Alt="Move documents"
                  Sequence="100"
                  Command="Invoke_MoveDocumentsRibbonCustomActionButtonRequest"
